<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/main1111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="환영합니다!"
        app:layout_constraintBottom_toTopOf="@+id/main1131"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/main1131"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="문제 풀러 가기"
        app:layout_constraintBottom_toTopOf="@+id/main1121"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main1111" />

    <Button
        android:id="@+id/main1121"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="통계(일단 버튼으로 만들었음)"
        app:layout_constraintBottom_toTopOf="@+id/main1141"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main1131" />

    <Button
        android:id="@+id/main1141"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="설정"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main1121" />

</androidx.constraintlayout.widget.ConstraintLayout>